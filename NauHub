local function HZNAI_fake_script() -- LoadingFrame.TweeningScript 
	local script = Instance.new('LocalScript', LoadingFrame)

	-- Script by Moses_chin2110
	local tween = script.Parent
	
	wait(2)
	
	if tween.Position == UDim2.new(0.5, 0,3, 0) then
		tween:TweenPosition(UDim2.new(0.5, 0, 0.5, 0),"Out","Quint")
	else
		tween:TweenPosition(UDim2.new(0.5, 0,3, 0),"In","Quint")
	end
	
end
