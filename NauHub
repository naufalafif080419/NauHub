local function KYCJO_fake_script() -- HomeButton.LocalScript 
	local script = Instance.new('LocalScript', HomeButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.ScriptFrame.Visible = false
		script.Parent.Parent.HomeFrame.Visible = true
		script.Parent.Parent.SettingsFrame.Visible = false
	end)
end
