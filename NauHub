local function XWMLIM_fake_script() -- ScriptLoadingButton.LocalScript 
	local script = Instance.new('LocalScript', ScriptLoadingButton)

	local tween = script.Parent.Parent
	
	
	
	script.Parent.MouseButton1Click:Connect(function()
		    local tween = script.Parent.Parent
			tween:TweenPosition((UDim2.new(0.5, 0, -0.7, 0)))
	
		
		
	end)
	
	
	script.Parent.MouseButton1Click:Connect(function()
		wait(3)
		script.Parent.Parent.Parent.MainFrame.Visible = true
	end)
	
	
end
